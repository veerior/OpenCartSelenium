package pavan_classes;

import java.util.Arrays;

public class Arrayconcept {

	
	public static void main(String[] args) {
		
		
		int[] arr={1,2,3,4,5};
		
		for (int a:arr) {System.out.println(a);};
		
		String[] name= {"Pratap","Sejal"};
		String n="Pratap";
		
		char c= n.charAt(0);
		System.out.println(c);

	}

}
