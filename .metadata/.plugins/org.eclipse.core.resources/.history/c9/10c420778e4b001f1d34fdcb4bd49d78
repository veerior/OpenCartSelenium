package pageObjects;

public class Practice3 {

	static int a=20;
	public static void tea() {
		System.out.println("practice3 method");
		int a=10;
		System.out.println(a);
		
	}
	
	public void coffee() {
		System.out.println("practice3 method");
		System.out.println(a);
	}
	
	class Bees { int b=50;}
	class Seeb extends Bees{ int b=25;}
	
	void sample() {
		int a=100;
		coffee();
		tea();
		Seeb s=new Seeb();
		System.out.println(s.b);
	}	
	
	
	
	public static void main(String[] args) {
		Practice3 pr=new Practice3();
		pr.tea();
		pr.coffee();
		System.out.println(pr.a);
		pr.sample();
		
	} 

}
